{
  "name": "eslint-plugin-logdna",
  "version": "2.0.0",
  "description": "ESlint plugin containing a collection of rules for enforcing code style at LogDNA",
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "extends": "semantic-release-config-logdna"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "logdna"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logdna/eslint-plugin-logdna.git"
  },
  "author": {
    "name": "LogDNA, Inc."
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/logdna/eslint-plugin-logdna/issues"
  },
  "homepage": "https://github.com/logdna/eslint-plugin-logdna#readme",
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "tap": "tap",
    "pretest": "npm run lint",
    "test": "tap",
    "release": "semantic-release"
  },
  "eslintConfig": {
    "root": true,
    "extends": "logdna",
    "ignorePatterns": [
      "node_modules/",
      "coverage/"
    ]
  },
  "dependencies": {
    "dot-prop": "^5.3.0",
    "is-builtin-module": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-logdna": "^4.0.2",
    "semantic-release": "^17.3.9",
    "semantic-release-config-logdna": "^1.1.0",
    "tap": "^14.10.8"
  }
}
